class Solution {
    public boolean hasPathSum(TreeNode root, int target) 
    {
        if(root==null){
            return false;
        }
        if(root.left==null && root.right==null){
            if(target-root.val==0){
                return true;
            }
            return false;
        }
    return hasPathSum(root.left,target-root.val) || hasPathSum(root.right,target-root.val);
 
    }
} 
